//determines if a student can graduate based on two grades input by user

#include <iostream>
#include <string>

using namespace std;

int main ()
{
    cout << "Please enter grades separated by a space: ";
    int grade1{};
    int grade2{};
    cin >> grade1 >> grade2;
    
    if (grade1 < 60 && grade2 <60)
        cout << "The student has failed.";
    else if (grade1 >= 95 && grade2 >= 95)
        cout << "The student has graduated with honors!";
    else
        cout << "The student has graduated.";
    
    return 0;
}
